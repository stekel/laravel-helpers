{
    "name": "stekel/laravel-helpers",
    "type": "library",
    "description": "A collection of Laravel helpers, blade directives, components, etc.",
    "license": "MIT",
    "homepage": "https://github.com/stekel/laravel-helpers",
    "keywords": [
        "Laravel",
        "helpers"
    ],
    "require": {
        "illuminate/support": "~5"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0",
    },
    "autoload": {
        "files": [
			"src/Debug.php",
			"src/Formatting.php",
			"src/General.php",
			"src/Random.php"
		]
    },
    "autoload-dev": {
        "psr-4": {
            "stekel\\LaravelHelpers\\Tests\\": "tests"
        }
    },
}